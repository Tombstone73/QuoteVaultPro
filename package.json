    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:dotenv": "node -r dotenv/config ./node_modules/drizzle-kit/bin.cjs migrate",
    "db:migrate:verbose": "tsx scripts/db-migrate-verbose.ts",
    "db:status": "tsx scripts/db-status.ts",
    "db:verify:stripe": "tsx scripts/db-verify-stripe.ts",
    "db:pbv2:check": "tsx scripts/db/pbv2-schema-check.ts",
    "db:sync-users": "tsx sync-users-customers.ts",
    "pbv2:smoke": "tsx scripts/pbv2-smoke.ts",
    "pbv2:http:smoke": "tsx scripts/pbv2-http-smoke.ts",
    "pdf:smoke": "tsx scripts/pdf-smoke.ts",
    "pdf:smoke:help": "tsx scripts/pdf-smoke.ts --help",
    "pdf:gen:local": "tsx scripts/pdf-gen-local.ts",
    "prepress:worker": "tsx scripts/prepress-worker.ts",
    "prepress:worker:dev": "tsx --watch scripts/prepress-worker.ts",
    "fix-user": "tsx fix-user-role.ts",
    "migrate:file-storage": "powershell ./scripts/run_file_storage_migration.ps1",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:client": "jest --config client/jest.config.js",
    "test:client:mem": "cross-env-shell \"NODE_OPTIONS=--max_old_space_size=4096\" jest --config client/jest.config.js",
    "test:smoke": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=smoke",
    "test:watch": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:watch:mem": "cross-env-shell \"NODE_OPTIONS=--max_old_space_size=4096 --experimental-vm-modules\" jest --watch"
  },
